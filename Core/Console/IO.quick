func Write(span Span.[Char]) Void
{
    Internal.CoreSystemConsoleWrite(span.data);
}

func WriteLine(span Span.[Char]) Void
{
    Internal.puts(span.data);
}

func WriteChar(char Char) Void
{
    Internal.CoreSystemConsoleWriteChar(char as Int32);
}

func Flush() Void
{
    Internal.CoreSystemConsoleFlush();
}